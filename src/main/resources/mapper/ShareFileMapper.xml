<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kang.mapper.ShareFileMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.kang.model.ShareFileDO">
        <id column="id" property="id"/>
        <result column="share_id" property="shareId"/>
        <result column="account_file_id" property="accountFileId"/>
        <result column="account_id" property="accountId"/>
        <result column="gmt_create" property="gmtCreate"/>
        <result column="gmt_modified" property="gmtModified"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, share_id, account_file_id, account_id, gmt_create, gmt_modified
    </sql>

</mapper>
